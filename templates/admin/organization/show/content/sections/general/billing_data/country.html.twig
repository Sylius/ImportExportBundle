{% set data = hookable_metadata.context.data %}

<tr>
    <th class="w-50">{{ 'sylius.ui.country'|trans }}</th>
    <td>
        {% if data.country is not empty %}
            {{ ux_icon('flagpack:' ~ data.country|lower, {'class': 'flag flag-xs'}) }}
            {{ data.country|sylius_country_name }}
        {% else %}
            {{ 'sylius_organization_plugin.ui.not_defined'|trans }}
        {% endif %}
    </td>
</tr>
